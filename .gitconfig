[user]
	name = Tomas Papan
	email = tomas.papan@gmail.com
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch 
        bra = branch -avv
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p

[color]
        ui = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[push]
	default = matching
[filter "media"]
	clean = git media clean %f
	smudge = git media smudge %f
	required = true
