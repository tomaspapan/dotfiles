alias docker-purge='docker stop $(docker ps -a -q) && docker rm $(docker ps -a -q) && docker rmi $(docker images -q)'
alias docker-shell=__docker-shell
alias docker-create='docker-machine create -d virtualbox --engine-storage-driver=overlay dev'
alias docker-clean='docker run --rm -v /var/run/docker.sock:/var/run/docker.sock spotify/docker-gc'

